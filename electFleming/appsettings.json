{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false,
        "SiteName": "Fleming For Putnam"
      },
      "Global": {
        "Id": "e3964562-9609-40b3-bd61-ab14675c91a8",
        "SanitizeTinyMce": true
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Error404Collection": [
          {
            "Culture": "en-US",
            "ContentXPath": "/Error"
          }
        ],
        "HideBackOfficeLogo": false,
        "LoginBackgroundImage": "",
        "LoginLogoImage": "/umbraco/assets/img/FlemingForPutnam_LogoWhiteOutline_Med.png",
        "Notifications": {
          "DisableHtmlEmail": false,
          "Email": "support@dalyweb.com"
        }
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://flemingforputnam.com/"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsDirectory": "~/../electFleming.Models"
      }
    }
  }
}